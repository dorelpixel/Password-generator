import random

letters = ('a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i')
letters1 = random.choice(letters)
letters2 = random.choice(letters)
letters3 = random.choice(letters)
letters4 = random.choice(letters)
numbers = (random.randrange(1, 1000))
password = str(letters1) + str(letters2) + str(letters3) + str(letters4) + str(
  numbers)
print(password)
